<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.resslen.projektbdio.DolaczEgzamin">

    <EditText
        android:id="@+id/etHasloEgzaminu"
        android:layout_width="354dp"
        android:layout_height="43dp"
        android:ems="10"
        android:hint="Haslo egzaminu"
        android:inputType="textPassword"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.081" />

    <Button
        android:id="@+id/bDolaczEgzamin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Dolacz do Egzaminu"
        android:layout_marginTop="9dp"
        app:layout_constraintTop_toBottomOf="@+id/etHasloEgzaminu"
        android:layout_marginLeft="70dp"
        app:layout_constraintLeft_toLeftOf="@+id/etHasloEgzaminu" />
</android.support.constraint.ConstraintLayout>
